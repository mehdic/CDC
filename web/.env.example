# ========================================
# MetaPharm Connect Web Application Environment Variables
# ========================================

# API Configuration
VITE_API_BASE_URL=http://localhost:4000
VITE_API_VERSION=v1
VITE_API_TIMEOUT=30000

# Environment
NODE_ENV=development

# Google Maps API (Delivery Tracking)
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Twilio Video (Teleconsultation)
VITE_TWILIO_VIDEO_API_KEY=your-twilio-video-api-key

# App Information
VITE_APP_NAME=MetaPharm Connect
VITE_APP_VERSION=0.1.0

# Feature Flags (Web)
VITE_ENABLE_TELECONSULTATION=true
VITE_ENABLE_E_COMMERCE=true
VITE_ENABLE_ANALYTICS_DASHBOARD=true
VITE_ENABLE_MESSAGING=true

# Sentry (Error Tracking)
VITE_SENTRY_DSN=your-sentry-dsn-web
VITE_SENTRY_ENVIRONMENT=development

# Analytics
VITE_ANALYTICS_ENABLED=false

# WebSocket Configuration
VITE_WEBSOCKET_URL=ws://localhost:4000

# Session Configuration
VITE_SESSION_TIMEOUT=3600000
VITE_SESSION_WARNING_TIMEOUT=300000
