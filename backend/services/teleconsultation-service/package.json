{
  "name": "@metapharm/teleconsultation-service",
  "version": "1.0.0",
  "description": "Teleconsultation Service with Twilio Video and AI transcription",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch"
  },
  "keywords": [
    "teleconsultation",
    "video",
    "twilio",
    "healthcare",
    "transcription"
  ],
  "author": "MetaPharm Connect",
  "license": "UNLICENSED",
  "dependencies": {
    "express": "^4.18.2",
    "better-sqlite3": "^12.4.1",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.15",
    "@types/better-sqlite3": "^7.6.8",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "jest": "^29.6.1",
    "@types/jest": "^29.5.4",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
